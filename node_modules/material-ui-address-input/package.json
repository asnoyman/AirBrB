{
  "name": "material-ui-address-input",
  "version": "0.4.0",
  "description": "A customisable address input component for Material-UI.",
  "main": "lib/AddressInput.js",
  "scripts": {
    "build": "babel src -d lib",
    "build-storybook": "build-storybook",
    "build:watch": "nodemon --watch src/ --exec npm run build",
    "deploy-storybook": "build-storybook -o storybook && gh-pages -d storybook && rm -r storybook",
    "prepare": "babel src -d lib",
    "storybook": "start-storybook -p 6006",
    "test": "standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanielArthurUK/material-ui-address-input.git"
  },
  "keywords": [
    "mui",
    "mui-org",
    "material-ui",
    "react",
    "address",
    "address-input",
    "react-input"
  ],
  "author": "DanielArthurUK",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DanielArthurUK/material-ui-address-input/issues"
  },
  "homepage": "https://github.com/DanielArthurUK/material-ui-address-input#readme",
  "devDependencies": {
    "@material-ui/core": "^1.4.3",
    "@material-ui/icons": "^2.0.1",
    "@storybook/addon-actions": "^3.4.10",
    "@storybook/addon-links": "^3.4.10",
    "@storybook/addons": "^3.4.10",
    "@storybook/react": "^3.4.10",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "gh-pages": "^1.2.0",
    "jest": "^23.4.2",
    "nodemon": "^1.18.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "standard": "^11.0.1"
  },
  "peerDependencies": {
    "@material-ui/core": "^1.0.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "lib/"
    ]
  },
  "dependencies": {
    "world-countries": "^2.0.0"
  }
}
